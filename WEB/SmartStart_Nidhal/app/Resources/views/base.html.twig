        <!DOCTYPE html>
        <html>
        <head>
            <meta charset="UTF-8" />
            <title>{% block title %}SmartStart Home{% endblock %}</title>

            {% block stylesheets %}

                <!-- Bootstrap core CSS -->
                <link href="{{ asset('vendor/bootstrap/css/bootstrap.css') }}" rel="stylesheet">

                <!-- Custom style for this template -->
                <link href="{{ asset('css/blog-home.css') }}" rel="stylesheet">

            {% endblock %}
            <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        </head>
        <body>
        {% block header %}
            <!-- develop the header of the page here -->
            <nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top">
                <div class="container">
                    <a class="navbar-brand" href="#">SmartStart</a>
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive"
                    <span class="navbar-toggler-icon"></span></button>
                    <div class="collapse navbar-collapse" id="navbarResponsive">
                        <ul class="navbar-nav ml-auto">
                            <li class="nav-item active">
                                <a class="nav-link" href="#">Home
                                    <span class="sr-only">(current)</span>
                                </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="#">About Us</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="#">Contact Us</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="#">Login</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
        {% endblock %}

        {% block content %}
            <!-- develop the content of the page here -->

        {% endblock %}

        {% block footer %}
            <!-- develop the footer of the page here -->
            <footer class="py-5 bg-dark">
                <div class="container">
                    <p class="m-0 text-center text-white">Copyright &copy; Team Epsilon 2019</p>
                </div>
            </footer>
        {% endblock %}

        {% block javascripts %} {% endblock %}
        </body>
        </html>
